
a{ text-decoration:none;}
.display-flex{display: -webkit-flex;display: flex;}
.flow-1{-webkit-flex: 1;flex: 1;}
.menu_box{ overflow-y:auto;}
.menuboxs{ width:100%; background-color:#fff; border-top:8px solid #f5f5f5; }
.menubox-title{ width:100%; height:40px; border-bottom:1px solid #e9e9e9;line-height:40px; padding-left:10px; color:#333; font-size:14px;}
.menus{ overflow:hidden; padding-bottom:15px;}
.menu{ float:left; width:20%; margin-left:4%;display:block; color:#333; padding-top:15px;}
.menu-icon{ width:55px; height:55px;  display:block; margin-left:auto; margin-right:auto;}
.menu-icon-box{ width:100%; text-align:center; position: relative;}
.menu-icon-number{position: absolute;
    right: 15%;
    top: -10px;
    margin-right: -10px;
    font-size: 12px;
    height: 20px;
    line-height: 20px;
    width: 20px;
    border-radius: 20px;
    background-color: red;
    color: #fff;
    text-align: center;}
.menu-text{ text-align:center; font-size:12px; margin-top:5px;}
.board_banner{ width:100%;}
.board_banner img{width:100%; display:block;}
.application_banner{ width: 100%; height: 120px; display: block;}
.menu-tips{
    width: 70%;
    position: fixed;
    left: 50%;
    margin-left: -35%;
    top: 45%;
    z-index: 9999999;
    text-align: center;
}
.menu-tips-bg{
    filter:alpha(opacity=0.7); /* IE */
    -moz-opacity:0.7; /* 老版Mozilla */
    -khtml-opacity:0.7; /* 老版Safari */
    opacity: 0.7; /* 支持opacity的浏览器*/
    background-color: #333;
    height: 100%;
   
}
.menu-tips-box{ position: relative;}
.menu-tips-bg{ position: absolute;top: 0px; bottom: 0px; width: 100%; border-radius: 6px; z-index: -1}
.menu-tips-text{ padding: 5px 12px; color: #fff; font-size: 13px; text-align: center;width: 100%}
.login-out{position: fixed; right: 10px; bottom: 40px; }
.login-out-icon{height: 50px; width: 50px; display: block;}
.remarks-fixed{ position: fixed; left: 0px; top: 0px; right: 0px; bottom: 0px;}
.remarks-relative{position: relative; width: 100%; height: 100%; justify-content: center; align-items: center;}
.over-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: absolute;left: 0px; top: 0px;}
.remarks-content{ background-color: #fff; position: relative; z-index: 1; width: 80%; border-radius:6px; margin-top: -10%;}
.remarks-title{height: 50px; line-height: 50px; text-align: center; color: #fff; background-color: #33b0ff; border-radius:6px 6px 0px 0px;font-size: 18px;font-weight: bold;}
.tip-text{ padding: 10px 10px; text-align: center;height: 85px;display: -webkit-flex;display: flex;-webkit-justify-content: center;justify-content: center;-webkit-align-items: center;align-items: center; color: #1a1a1a;font-size: 14px;}
.tip-btns{border-top:1px solid #E9E9E9; padding: 0px 10px;}
.cancel-btn,.comfire-btn{ text-align: center; height: 45px; line-height: 45px;font-size: 16px;font-weight: bold;}
.cancel-btn{border-right:1px solid #E9E9E9;color: #999;}
.comfire-btn{ color: #33b0ff;}


.emtry-view{text-align: center;border-top:8px solid #f5f5f5;}
.emtry-view image{width: 80px; height: 100px;margin-top: 100px;margin-bottom: 20px;}
.emtry-view text{color: #999999;font-size: 16px;}

.certification-box{width: 100%;height:100%;position: fixed;bottom: 0;}
.bg{background: black;opacity: 0.2;width: 100%;height:100%;}
.c-box{width: 100%;padding:20px 0;position: fixed;bottom: 0;text-align: center;background: white;}
.c-box image:nth-child(2){width: 42px;height: 50px;}
.c-text{max-width: 80%;margin: auto;padding: 10px 0;}
.c-text text{font-size: 16px;}
.btn-c{background: #33b0ff;color: white;border-radius: 15px;width: 150px;height: 30px;line-height: 30px;font-size: 14px;margin: auto;}
.c-close{position: absolute;right: 10px;top: 10px;width: 20px;height: 20px;}

.d-box{width: 80%;height:400px;border-radius:6px;bottom: 0;background: white;position: fixed;top:50%;margin-top:-200px;left: 50%;margin-left: -40%;}
.d-title{position: relative;width: 100%;padding: 12px 10px;background: #f0f0f0;border-top-left-radius:6px;border-top-right-radius:6px;}
.d-title>image{width: 40px;height: 40px;}
.d-title>image:last-child{width: 20px;height: 20px;position: absolute;right: -8px;top: -8px;}
.d-title>view{line-height: 40px;font-size: 16px;font-weight: bold;color: #1a1a1a;margin-left: 10px;}
.d-photo{text-align: center;margin: 10px;}
.d-photo>image{width:250px;height: 250px;}
.d-photo>view{font-size: 14px;color: #999999;}
.d-option{color: #33b0ff;text-align: center;font-size: 16px;line-height: 40px;}
.btn-share{border: none !important;color: #33b0ff !important;font-size: 16px !important;padding: 0px !important;}