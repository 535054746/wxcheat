/* pages/customerManagement/Addcustomer/Addcustomer.wxss */
.clear-image image{width: 14px;height: 14px;line-height: 45px; text-align: right;padding-right: 12px}
.lines{background-color:#f5f5f5 ;height: 1px;margin-left: 12px;  margin-right: 12px;}
.input-change{line-height: 45px;height: 45px;padding-left: 12px;padding-right: 12px}
.input-change2{padding-left: 12px;padding-right: 12px; padding-top: 13px; padding-bottom: 13px;}
.change-btn{margin-left: 12px;margin-right: 12px;background-color: #33b0ff;color: white; display: block}
.button-box{ background-color: #fff;position: fixed;left: 0px;bottom: 0px;right: 0px;margin-bottom: 12px}
.store-name{color: #666666;margin-left: 12px;width: 60px; height: 45px; line-height: 45px;}
.add-image image{width: 13px;height: 14px;line-height: 15px; text-align: right;}
.contacts{height: 45px}
.contacts-name{height: 45px;line-height: 45px;margin-left: 12px;color: #666666;width: 70px}
.contacts-input{height: 45px;line-height: 45px;margin-left: 12px}
.choose-contacts{ text-align: center;color: #33b0ff}
.add-image{text-align: center;width: 100%; margin-top: 25px;}
.select-contacts{ width: 120px; border-left:1px solid #f5f5f5; height: 90px; padding-right: 12px;overflow: hidden;}
.bot-text{color: #b3b3b3;margin-left: 12px;size: 12px;margin-top: 9px;}
.bot-text text{color: red}
.ck_tips{position: fixed; left: 50%;top: 40%;z-index: 100000; margin-left: -100px; margin-top: -18px;}
.tips_box{position: relative;width:200px;}
.tips_bg{ width: 100%; height: 100%; background-color: #000;opacity: 0.6;border-radius: 8px;position: absolute;z-index: -1;}
.tips_text{ width: 100%; height: 100%;text-align: center; line-height: 20px; color: #fff; font-size: 14px; padding: 5px 12px;}
.select-icon image{ width: 8px; height: 8px;}
.company-item{ height: 45px; line-height: 45px; padding: 0px 12px; border-top: 1px solid #f5f5f5; color: #666;}
.company-box{ position: fixed; top: 46px; left: 0px; right: 0px; background-color: #fff; max-height: 300px; overflow-y: auto; font-size: 14px; z-index: 1000000000}
.store-clear{ padding-right: 12px; margin-top: 13px;}
.store-clear image{ width: 18px; height: 18px;}
.over-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: fixed;left: 0px; top: 46px;}
.money_total{ position: fixed;width: 100%; left: 0px; bottom: 0px;height: 50px; background-color: #fff;}
.money_total_box{ text-align: right; padding:5px 12px; border-top:1px solid #e3e3e3;}
.sure-button{ background-color: #33b0ff; color: #fff; text-align: center; line-height: 40px; height: 40px; border-radius: 6px; font-size: 16px; font-weight: bold;}
.loading-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: fixed;left: 0px; top: 0px;}
.loading-icon{ width: 100%; height: 100%;position: fixed;left: 0px; top: 0px; justify-content: center; align-content: center; align-items: center;}
.loading-icon image{ width: 30px; height: 30px;}
.color-tip{ color: #b3b3b3;}