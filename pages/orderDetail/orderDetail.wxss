.order_detail_info{ padding: 0px 10px; background-color: #fff;}
.padding-olo{ padding: 0px 10px;}
.padding-bot-50{ padding-bottom: 50px;}
.info-text{ width: 80px; color: #999;}
.info-line{ height: 45px; line-height: 45px;border-bottom: 1px solid #eee;}
.info-value{ color: #666;}
.statu-color{color:#33b0ff;}
.info-show{ text-align: center;}
.info-show{padding: 10px 0px;}
.up_ion{ width:19px; height: 10px; }
.order_title{ background-color: #fff; margin-top: 8px;padding: 0px 10px; height: 45px; line-height: 45px; margin-bottom: 5px;}
.title-text{ overflow: hidden;color: #1a1a1a;}
.title-icon{ width: 15px ;height: 20px; margin-top: 12px; margin-right: 5px;}
.title-number{ color: #999;}
.order-item{background-color: #fff; margin-bottom: 8px;}
.order-item-top{padding: 0px 10px;border-bottom: 1px solid #e9e9e9; padding-bottom: 10px;}
.shop-name-box{ height: 45px; line-height: 45px;}
.shop-name{ font-weight: bold; color: #1a1a1a;}
.border-left-icon{ border-left: 6px solid #33b0ff; height: 17px; margin-right: 5px; margin-top: 15px;}
.shop-number-icon,.shop-weight-icon{ width:12px; height: 7px; margin-left: 5px; }
.shop-number{ color: #999;}
.address-icon{ width: 16px; height: 19px; vertical-align: middle; margin-top: -3px; margin-right: 8px;}
.address-text{ color: #666;}
.order-item-bottom{ padding: 0px 10px; height: 45px; line-height: 45px;}
.look-remarks{ color: #33b0ff;}
.look-remarks-line{border-left:1px solid #ccc; height: 23px; margin-left: 10px; margin-right: 10px;width:1px; margin-top: 10px;}
.deliver-button{ width: 50px; height: 29px; float: right; font-size: 14px; padding: 0px 0px; margin-top: 8px; background-color: #33b0ff; color: #fff; line-height: 29px;border: 0px;}
.shop-type{ border-top:8px solid #eee;border-bottom: 1px solid #e9e9e9;}
.shop-type-title{ height: 45px; line-height: 45px; padding: 0px 10px;}
.shop-type-detail-title{ height: 45px; line-height: 45px;}
.shop-weight-text{ color: #33b0ff}
.shop-type-name{ font-weight: bold; color: #1a1a1a;}
.shop-type-deital-name{ color: #1a1a1a; flex: 2;-webkit-box-flex: 2;}
.shop-type-line{ position:relative; height: 20px;}
.type-line-border{ height: 10px;border-bottom: 1px solid #33b0ff;}
.type-line-text-box{ position: absolute; left: 0px;top: 0px; justify-content: center; align-items: center;width:100%;}
.type-line-text{ height: 20px; background-color: #33b0ff; color: #fff; border-radius: 10px; padding: 0px 10px;}
.bg-blue{ background-color: #f8fdff;}
.distribution-box{ color: #666; height: 40px; line-height: 40px; border-top:1px solid #f2f2f2;}
.distribution-detail{ font-size: 12px; color: #b3b3b3; border-top: 1px solid #fbfbfb;line-height: 20px; padding: 8px 0px;}
.deliver-box{ position: fixed; bottom: 0px; left: 0px; right: 0px;height: 50px; background-color: #fff;}
.deliver-big-button{ margin-left:10px;margin-right:10px;height: 36px; line-height: 36px; background-color: #33b0ff; color:  #fff; margin-top: 7px; font-size: 14px;}
.remarks-fixed{ position: fixed; left: 0px; top: 0px; right: 0px; bottom: 0px;}
.remarks-relative{position: relative; width: 100%; height: 100%; justify-content: center; align-items: center;}
.over-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: absolute;left: 0px; top: 0px;}
.remarks-content{ background-color: #fff; position: relative; z-index: 1; width: 80%; border-radius:6px;}
.remarks-title{height: 40px; line-height: 40px; text-align: center; color: #fff; background-color: #33b0ff; border-radius:6px 6px 0px 0px;}
.remarks-shop-name{ padding: 0px 10px; color: #33b0ff; height: 45px; line-height: 45px; border-bottom: 1px solid #e9e9e9;}
.remarks-phone-icon{ width: 18px; height: 18px; margin-top: 12px;}
.department-remarks{ height: 40px; line-height: 40px; padding: 0px 10px;}
.autio_button{ width: 25px; height: 25px; vertical-align: middle; margin-right: 5px;}
.autio_progress{ width: 12px; height: 9px; vertical-align: middle; margin-right: 5px}
.autio-middle{ vertical-align: middle;}
.remarks-detail{padding: 10px 10px; margin-left: 10px; margin-right: 10px; background-color: #f5f5f5;border: 1px solid #f0f0f0;border-radius:6px;}
.remarks-detail-autio{ margin-bottom: 10px; color: #33b0ff;}
.remarks-item{ padding-bottom: 10px;border-bottom: 5px solid #eee;border-radius: 0px 0px 5px 5px;}
.scroll-remarks{ overflow-y: auto;max-height: 300px;}
.remarks-close-icon{ position: absolute;right: -10px; top: -10px; width: 20px; height: 20px;}
.bottom-white-50{ height: 50px; background-color: #fff;}
.order-item-bottom-statu{ text-align: right;}
.statu-color-gray{ color: #ccc;}
.shop-weight-book{ flex: 1; -webkit-box-flex: 1; color: #999;}
.shop-weight-real{ flex: 1; -webkit-box-flex: 1; color: #33b0ff;}
.book-icon,.real-icon{ width: 14px; height: 14px; vertical-align: middle;margin-top: -3px; margin-right: 5px;}
.color-red{color: red;}
.text-right{text-align: right;}
.order-item-time{font-size:11px;color: #999;padding: 8px 10px;border-top: 1px solid #e9e9e9; height: 36px; line-height: 20px;}
.padding-right-17{padding-right:17px;}
.mb50{ margin-bottom: 50px;}
