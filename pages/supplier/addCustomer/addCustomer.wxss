.container{ border-top:1px solid #f5f5f5}
.order_tab_five{ background-color: #fff; position: fixed;left: 0px;bottom: 0px;right: 0px; padding:5px 12px; display: flex; height: 52px; line-height: 40px;border-top: 1px solid #e9e9e9;}
.store-icon image{ width: 20px; height: 20px; margin-top: 10px;}
.order_sure_icon{ margin-right: 10px;}
.order_sure_icon image{ width: 20px; height: 20px; margin-top: 15px;}
.create-botton-sure{border:1px solid #33b0ff;border-radius: 6px; color: #fff; background-color: #33b0ff; padding: 0px 12px; flex: 1;margin-left: 10px; text-align: center; font-size: 16px; font-weight: bold;height: 40px;}
.order_all{ color: #999;}
.store-item{margin: 0px 12px; height: 40px; line-height: 40px; display: flex;border-bottom: 1px solid #f5f5f5; background: #fff;}
.store-name{flex: 1;}
.store-name view{overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap; width: 200px;}
.store-icon image{ width: 20px; height: 20px; margin-top: 10px; margin-right: 10px;}
.store-statu{ color: #aaa !important;}
.ck_tips{position: fixed; left: 50%;top: 40%;z-index: 100000; margin-left: -100px; margin-top: -18px;}
.tips_box{position: relative;width:200px; min-height: 30px; }
.tips_bg{ width: 100%;  background-color: #000;opacity: 0.6;border-radius: 8px;position: absolute}
.tips_text{ width: 100%; text-align: center; line-height: 30px;position: absolute;z-index: 10000000000000; color: #fff;background-color: #000;max-height: 90px;overflow: hidden;border-radius: 8px;}

.loading-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: fixed;left: 0px; top: 0px;}
.loading-icon{ width: 100%; height: 100%;position: fixed;left: 0px; top: 0px; justify-content: center; align-content: center; align-items: center;}
.loading-icon image{ width: 30px; height: 30px;}