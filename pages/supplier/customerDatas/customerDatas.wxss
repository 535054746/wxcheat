.company-info{ margin-bottom: 5px;}
.company-box{ padding: 8px 12px; background-color: #fff; border-bottom: 1px solid #f5f5f5; border-top:1px solid #f2f2f2;}
.company-image image{ width: 50px; height: 50px; display: block;}
.company-name{ padding-left: 10px;}
.company-name-text{ margin-top: 5px;}
.company-name-time{ color: #999;}
.company-number{ height: 50px; line-height: 50px; color: #999;}
.doing-time{ height: 30px; line-height: 30px; padding: 0px 12px; background-color: #fff; margin-bottom: 5px; color: #888;}
.item-box{ background-color: #fff; margin-bottom: 5px;}
.items{ padding: 0px 12px; height: 40px; line-height: 40px;}
