.sure-button{ background-color: #33b0ff; color: #fff; text-align: center; line-height: 40px; height: 40px; border-radius: 6px; font-size: 16px;}
.total-number{background-color: #fff; height: 45px; line-height: 45px; padding: 0px 12px;position: fixed;bottom: 0px;left: 0px; right: 0px;border-top:1px solid #f2f2f2;}
.te-ri{ text-align: right;}
.total-number view text{ color: #33b0ff;}
