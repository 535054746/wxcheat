.order-main{ border-top:1px solid #f2f2f2; position: fixed; top: 0px; left: 0px; right: 0px;}
.type-header{ padding: 0px 12px; background-color: #fff; display: flex;}
.type-header-box{ overflow-y: auto;}
.current-type{color: #33b0ff}
.down-icon{ width: 18px; height: 18px;margin-top: 30px; margin-left: 10px;}
.search-icon{ width: 18px; height: 18px;margin-top: 14px; margin-left: 10px;}
.shop-type{ padding: 0px 12px; color: #ee8c41; font-size: 13px;margin-top: 5px;}
.shop-item{ margin-top: 5px;}
.shop-item-image{width: 80px; height: 80px;}
.shop-item-image image{ width: 80px; height: 80px; display: block;}
.shop-item{ padding: 12px 12px; background-color: #fff;}
.shop-name-text{ margin-top: 5px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap; width: 140px;}
.shop-item-name{ padding: 0px 12px;}
.shop-name-unit{ margin-top: 25px; font-size: 13px; color: #999;}
.edit-icon{}
.delete-icon{width:28px;}
.money_total{ position: fixed;width: 100%; left: 0px; bottom: 0px;height: 50px; background-color: #fff;}
.money_total_box{ text-align: right; padding:5px 12px; border-top:1px solid #e3e3e3;}
.sure-button{ background-color: #33b0ff; color: #fff; text-align: center; line-height: 40px; height: 40px; border-radius: 6px; font-size: 16px;}
.type-header-box-box{ width: 10000px;}
.type-header-box-box-box{ float: left; overflow: hidden;}
.type-header-box-box-box view{height: 45px; line-height: 45px; text-align: center; float: left; margin-right: 20px;}
.padding-tb{ padding-top: 46px; margin-bottom: 50px;}
.search-box{ background-color: #fff; padding: 0px 12px;height: 45px; line-height: 45px; overflow: hidden; position: fixed;top: 0px;right: 0px; left: 0px;}
.search-input{ position: relative; margin-top: 10px;}
.search-input input{ background-color: #f5f5f5; border-radius: 6px; margin-right: 12px; height: 25px; padding: 0px 12px;}
.search-input image{ position: absolute; right: 20px; top: 4px; z-index: 1;width: 18px; height: 18px;}
.cancel-button{ color: #33b0ff;}

.remarks-fixed{ position: fixed; left: 0px; top: 0px; right: 0px; bottom: 0px;}
.remarks-relative{position: relative; width: 100%; height: 100%; justify-content: center; align-items: center;}
.over-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: absolute;left: 0px; top: 0px;}
.remarks-content{ background-color: #fff; position: relative; z-index: 1; width: 80%; border-radius:6px; margin-top: -10%;}
.remarks-title{height: 50px; line-height: 50px; text-align: center; color: #fff; background-color: #33b0ff; border-radius:6px 6px 0px 0px;font-size: 14px;}
.tip-text{ padding: 10px 10px; text-align: center;height: 85px;display: -webkit-flex;display: flex;-webkit-justify-content: center;justify-content: center;-webkit-align-items: center;align-items: center; color: #1a1a1a;font-size: 13px;}
.tip-btns{border-top:1px solid #e1e1e1; padding: 0px 10px;}
.cancel-btn,.comfire-btn{ text-align: center; height: 45px; line-height: 45px;font-size: 13px;}
.cancel-btn{border-right:1px solid #e1e1e1;color: #999;}
.comfire-btn{ color: #33b0ff;}
.loading-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: fixed;left: 0px; top: 0px;}
.loading-icon{ width: 100%; height: 100%;position: fixed;left: 0px; top: 0px; justify-content: center; align-content: center; align-items: center;}
.loading-icon image{ width: 30px; height: 30px;}

.ck_tips{position: fixed; left: 50%;top: 40%;z-index: 100000; margin-left: -100px; margin-top: -18px;}
.tips_box{position: relative;width:200px;}
.tips_bg{ width: 100%; height: 100%; background-color: #000;opacity: 0.6;border-radius: 8px;position: absolute;z-index: -1;}
.tips_text{ width: 100%; height: 100%;text-align: center; line-height: 20px; color: #fff; font-size: 14px; padding: 5px 12px;}
.no-data-tip{padding-top: 100px; text-align: center; color: #999; background-color: #fff;}
.no-data-tip view{ margin-top: 5px;}
.no-data-image image{ width: 60px; height: 75px;}
.no-data-image2 image{ width: 60px; height: 60px;}
.item-rule{ padding: 5px 12px; border-top: 1px solid #f9f9f9; color: #b5b5b5; font-size: 13px; background-color: #fff; height:35px; line-height: 25px;}
.paddingT100{padding-top:100px;}
.add-button{ padding: 0px 12px; color:  #fff; border-radius: 3px; border:1px solid #33b0ff; text-align: center; margin-top: 30px; height: 25px; line-height: 22px; background-color: #33b0ff;}
.add-button2{ padding: 0px 12px; color:  #fff; border-radius: 3px; border:1px solid #33b0ff; text-align: center;height: 25px; line-height: 22px; background-color: #33b0ff;}
.add-button3{ padding: 0px 12px; color:  #fff; border-radius: 3px; border:1px solid #ccc; text-align: center;height: 25px;margin-top: 30px; line-height: 22px; background-color: #ccc;}
.add-button4{ padding: 0px 12px; color:  #fff; border-radius: 3px; border:1px solid #ccc; text-align: center;height: 25px; line-height: 22px; background-color: #ccc;}