.flex1{ flex: 1;}

.shop-item{ background-color: #fff; margin-bottom: 5px;}
.item-choose{ width: 20px; height: 20px;}
.shop-name{ margin-left: 5px;}
.item-delete,.item-delete{ width: 20px;height: 20px; margin-top: 2px;}
.weight{ width: 100px; height: 25px; line-height: 25px;}
.weight text{ color: #33b0ff;}
.shop-item-box{ padding: 12px 12px;}
.shop-item-box2{ padding: 5px 12px; padding-right: 32px;}
.weight input{border:1px solid #e9e9e9;border-radius: 6px;text-align: center; display: block; width: 50px; margin-right: 5px; height: 20px;}
