.select-center{ position: fixed; left: 0px; top: 0px; bottom: 0px; right: 0px; border-top: 1px solid #e9e9e9;}
.select-center-left{ width: 100px; background-color: #f5f5f5;overflow-y: auto;}
.select-center-left view{ height: 45px; line-height:  45px; text-align: center; border-bottom: 1px solid #e9e9e9; color: #666;}
.current-type{ color: #33b0ff !important; background-color: #fff; font-weight: bold;}
.select-center-right{ padding-left:12px;padding-right:12px; overflow-y: auto;}
.shop-type-item{ height: 45px; line-height: 45px; border-bottom: 1px solid #f5f5f5;}
.shop-type-item-icon image{ width: 18px; height: 13px;}