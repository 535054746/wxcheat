.item-box{ padding: 0px 12px; background-color: #fff; height: 45px; line-height: 45px; margin-bottom: 10px;}
.item-name-icon image{ width: 7px; height: 15px;}
.item-title{ color: #999; padding-right: 20px;}
.item-name-text{ color: #666;}
.no-margin{ margin-bottom: 0px !important;}
.border-bottom{ border-bottom: 1px solid #e9e9e9;}
.money_total{ position: fixed;width: 100%; left: 0px; bottom: 0px;height: 50px; background-color: #fff;}
.money_total_box{ text-align: right; padding:5px 12px; border-top:1px solid #e3e3e3;}
.sure-button{ background-color: #33b0ff; color: #fff; text-align: center; line-height: 40px; height: 40px; border-radius: 6px; font-size: 16px; font-weight: bold;}
.shop-rule{ background-color: #fff;}
.shop-rule-title{ height: 45px; line-height: 45px; border-bottom: 1px solid #e9e9e9; padding: 0px 12px; color: #999;}
.shop-rule-content{ padding: 12px 12px;}
.delete-icon{ height: 46px; line-height: 46px; margin-left: 12px;}
.delete-icon image{ width: 25px; height: 25px; margin-top: 10px;}
.shop-rule-item{ background-color: #f5f5f5; border-radius: 6px; border:1px solid #e9e9e9;}
.rule-item-name{ height: 45px; border-right:1px solid #e9e9e9; line-height: 45px; width: 80px; text-align: center;overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;}
.rule-item-text{ height: 45px; line-height: 45px;padding: 0px 12px; flex: 1; color: #999;position: relative;}
.rule-item-text image{ position: absolute; right: 12px; top: 15px; height: 15px; width: 7px;}
.add-rule{ padding: 0px 12px; color: #33b0ff;}
.add-rule image{ width: 20px; height: 20px; vertical-align: middle; margin-top: -3px; margin-right: 5px;}
.remarks-fixed{ position: fixed; left: 0px; top: 0px; right: 0px; bottom: 0px; z-index: 100}
.remarks-relative{position: relative; width: 100%; height: 100%; justify-content: center; align-items: center;}
.over-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: absolute;left: 0px; top: 0px;}
.remarks-content{ background-color: #fff; position: relative; z-index: 1; width: 80%; border-radius:6px; margin-top: -10%;}
.remarks-title{height: 50px; line-height: 50px; text-align: center; color: #fff; background-color: #33b0ff; border-radius:6px 6px 0px 0px; position: relative; font-size: 16px; font-weight: bold;}
.remarks-title view{ width: 20px; height: 20px; position: absolute; right: -12px; top: -18px;}
.remarks-title view image{width: 20px; height: 20px; }
.tip-text{ padding: 10px 10px; text-align: center;height: 85px;display: -webkit-flex;display: flex;-webkit-justify-content: center;justify-content: center;-webkit-align-items: center;align-items: center; color: #1a1a1a;font-size: 13px;}
.tip-btns{border-top:1px solid #e1e1e1; padding: 0px 10px;}
.cancel-btn,.comfire-btn{ text-align: center; height: 45px; line-height: 45px;font-size: 13px;}
.cancel-btn{border-right:1px solid #e1e1e1;color: #999;}
.comfire-btn{ color: #33b0ff;}
.shopname-input{ height: 45px; line-height: 45px;}
.shop-type-item{ height: 45px; line-height: 45px; padding: 0px 12px;}
.shop-type-item-icon image{ width: 18px; height: 13px;}
.no-bottom{border-bottom: 0px;}
.shop-type-item-box{height: 300px; overflow-y: auto;}
.ck_tips{position: fixed; left: 50%;top: 40%;z-index: 100000; margin-left: -100px; margin-top: -18px;}
.tips_box{position: relative;width:200px; height: 30px;}
.tips_bg{ width: 100%; height: 100%; background-color: #000;opacity: 0.6;border-radius: 8px;position: absolute}
.tips_text{ width: 100%; height: 100%;text-align: center; line-height: 30px;position: absolute;z-index: 10000000000000; color: #fff; font-size: 14px;}
.mrb{margin-bottom: 60px;}
.shop-type-item-text{ font-size: 14px; border-bottom: 1px solid #f5f5f5;}
.font-tip{ color: #b3b3b3;}
.unit-tip-color{color: #b3b3b3;}
.selectBg{ background-color: #f5f5f5;}