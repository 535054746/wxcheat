.select-main{ position: fixed;left: 0px; right: 0px; bottom: 0px; top: 0px; z-index: 2}
.body-bg{ position: fixed;left: 0px; right: 0px; bottom: 0px; top: 0px; z-index: 1; display: flex;}

.logo_box{
  width: 100%;
  height: 100%
}
.logo{
  width: 100%;
  height: 100%;
}

.select-button{ margin-top: 100px;}
.select-button view{padding: 0px 12px; text-align: center; color:  #fff; border:1px solid #fff; border-radius: 30px;height: 30px; line-height: 28px; width: 140px; margin-left: auto; margin-right: auto; margin-top:20px;}
.copyright-box{text-align: center;position: fixed;left: 0;right: 0;bottom: 30px;color: #B3B3B3;}
.copyright-box .name{font-size: 16px;}
.copyright{font-size: 14px;margin-top: 3px;}

.remarks-fixed{ position: fixed; left: 0px; top: 0px; right: 0px; bottom: 0px;z-index: 2;}
.remarks-relative{position: relative; width: 100%; height: 100%; justify-content: center; align-items: center;}
.over-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: absolute;left: 0px; top: 0px;}
.remarks-content{ background-color: #fff; position: relative; z-index: 1; width: 80%; border-radius:6px; margin-top: -10%;}
.remarks-title{height: 50px; line-height: 50px; text-align: center; color: #fff; background-color: #33b0ff; border-radius:6px 6px 0px 0px;font-size: 18px;font-weight: bold;}
.tip-text{ padding: 10px 10px; text-align: center;height: 85px;display: -webkit-flex;display: flex;-webkit-justify-content: center;justify-content: center;-webkit-align-items: center;align-items: center; color: #1a1a1a;font-size: 14px;}
.tip-btns{border-top:1px solid #e1e1e1; padding: 0px 10px;}
.cancel-btn,.comfire-btn{ text-align: center; height: 45px; line-height: 45px;font-size: 16px;font-weight: bold;}
.cancel-btn{border-right:1px solid #e1e1e1;color: #999;}
.comfire-btn{ color: #33b0ff;}

.remarks-title{position: relative;}
.line-f7{height: 1px;background: #f7f7f7;}
.shareItem{ height: 47px;line-height: 47px;font-size: 16px;}
.shareName{width: 90px;color: #666666;padding-left: 12px;}
.shareValue{color: #1a1a1a;}
.shareBtn{width: 152px;height: 34px;font-weight: bold;font-size: 16px;color: white;background: #33b0ff;margin: 12px auto;border-radius: 17px;text-align: center;line-height: 34px;}
.remarks-title>image{position: absolute;top: -5px;right: -5px;width: 20px;height: 20px;}