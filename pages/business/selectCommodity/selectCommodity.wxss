.search-top{ position: fixed; top: 0px; left: 0px; right: 0px; height: 45px; border-bottom: 1px solid #e9e9e9;}
.search-input-box{ margin-left: 12px; position: relative; margin-right: 12px; margin-top: 6px;}
.search-input-box input{ background-color: #f5f5f5; border:1px solid #e9e9e9; border-radius: 5px; height: 30px; line-height: 30px; padding-left: 12px; padding-right: 40px;}
.search-icon{ width: 18px; height: 18px; position: absolute;right: 7px; top: 7px; z-index: 10;}
.search-text{ height: 30px; line-height: 30px; margin-right: 12px; color: #33b0ff; margin-top: 6px;}
.money_total{ position: fixed;width: 100%; left: 0px; bottom: 0px;height: 50px; background-color: #fff;}
.money_total_box{ text-align: right; padding:5px 12px; border-top:1px solid #e3e3e3;}
.sure-button{ background-color: #33b0ff; color: #fff; text-align: center; line-height: 40px; height: 40px; border-radius: 6px; font-size: 16px;}
.select-center{ position: fixed; left: 0px; top: 45px; bottom: 50px; right: 0px;}
.select-center-left{ width: 100px; background-color: #f5f5f5;overflow-y: auto;}
.select-center-left view{ height: 45px; line-height:  45px; text-align: center; border-bottom: 1px solid #e9e9e9; color: #666;}
.current-type{ color: #33b0ff !important; background-color: #fff;}
.select-center-right{ padding-left:12px; overflow-y: auto;}
.item-type{ font-size: 13px; margin-top: 14px; margin-right: 12px;}
.item-type-name{ background-color: #33b0ff; color: #fff; border-radius: 10px; padding: 0px 12px;}
.item-type-line{ height: 10px; border-bottom: 1px solid #33b0ff;}
.item-type-button image{ width: 20px; height: 20px; vertical-align: middle; margin-top: -3px; margin-right: 5px;}
.item-type-button{ color: #33b0ff;}
.shop-item{ margin-top: 14px;}
.shop-item-line{ height:10px;  border-bottom: 5px solid #f5f5f5; margin-right: 12px;}
.shop-item-box{ margin-bottom: 7px;}
.shop-item-box view{ width: 30%; float: left; text-align: center;border:1px solid #e9e9e9; margin-right:12px; border-radius: 6px; height: 28px; line-height: 26px;}
.shop-rule-box{ position: fixed; bottom: 0px; left: 0px; right: 0px; background-color: #fff;}
.shop-rule-title{ padding: 0px 12px; border-bottom: 1px solid #e9e9e9; height: 45px; line-height: 45px;}
.shop-rule-icon image{ width: 20px; height: 20px; vertical-align: middle; margin-top: -3px;}
.shop-rule-unit{ height: 45px; line-height: 45px; border-bottom: 1px solid #f9f9f9; margin-left: 12px; margin-right: 12px;}
.shop-rule-unit-input input{border:1px solid #e9e9e9; border-radius: 6px; background-color: #f5f5f5; vertical-align: middle; margin-top: -3px; display: inline-block; width: 70px; margin-left: 10px; text-align: center;}
.shop-rule-unit-input text{ color: #666;}
.tr{ text-align: right;}
.rule-tile-box{ height: 45px; line-height: 45px; padding: 0px 12px;color: #666;}
.color-33b0ff{ color: #33b0ff;}
.rule-item-box{ padding-right:12px; overflow: hidden;}
.rule-item-box view{ background-color: #f5f5f5; border-radius: 20px; float: left; padding: 0px 10px; height: 25px; line-height: 23px; margin-left: 12px; margin-bottom: 10px;}
.delet-rule-icon image{ width: 25px; height: 25px; vertical-align: middle; margin-top: 0px;}
.add-rule-box{padding: 0px 12px; margin-bottom: 10px;}
.add-rule-input{ border:1px solid #33b0ff; border-radius: 20px; height: 28px; margin-right: 12px;}
.add-rule-input input{ height: 26px; padding: 0px 8px;}
.add-rule-sure{ background-color: #33b0ff; color: #fff; text-align: center; height: 26px; line-height: 26px; padding: 0px 18px; border-radius: 30px;}
.rule-box{ border-bottom: 1px solid #e9e9e9;}
.add-rule-line{ border-bottom: 1px solid #e9e9e9; margin-left: 12px; margin-right: 12px;}
.add-rule-comfire{background-color: #33b0ff; text-align: center; height: 45px; line-height: 45px; color: #fff; margin-top: 10px; font-size: 15px;}
.over-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: fixed;left: 0px; top: 0px; bottom: 0px; right: 0px;}
.shop-item-more{ border:0px !important;}
.shop-item-more image{ width: 15px; height: 7px;}
.subtract{ border-radius: 6px 0px 0px 6px;border:1px solid #e9e9e9; background-color: #f5f5f5; width: 28px; line-height: 22px; height: 28px; text-align: center; font-size: 28px;}
.add{ border-radius: 0px 6px 6px 0px;border:1px solid #e9e9e9; background-color: #f5f5f5; width: 28px; line-height: 22px; height: 28px; text-align: center; font-size: 22px;}
.number-input input{ width: 40px; height: 25px; border:1px solid #e9e9e9; border-left: 0px; border-right: 0px; text-align: center;}
.number-input{ height: 28px;}
.number-input-box{ margin-top: 10px;}
