@import "../common/page/footer.wxss";
.select-icon image{ width: 25px; height: 25px;vertical-align: middle; margin-top: -3px; margin-right: 10px;}
.select-image image{ width: 25px; height: 25px;vertical-align: middle; margin-top: 25px; margin-right: 10px;}
.more-icon image{ width: 7px; height: 15px; vertical-align: middle; margin-top: -3px;}
.item-title{ height: 45px; line-height: 43px; background-color: #fff; padding: 0px 12px; border-bottom: 1px solid #e9e9e9;  }
.shop-item-overflow{width:100%; overflow: hidden;}
.shop-item{ border-bottom: 5px solid #f5f5f5;}
.shop-item-image{width: 80px; height: 80px;}
.shop-item-image image{ width: 80px; height: 80px; display: block;border-radius: 6px;}
.shop-item{ padding: 12px 12px; background-color: #fff;}
.shop-name-text{ margin-top: 5px;height: 52px;text-align: justify;line-height: 136%;}
.shop-name-text text{display: -webkit-box;font-weight: bold;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:3;
    overflow: hidden;
    text-overflow:ellipsis;}
.shop-item-name{ padding: 0px 12px;}
.shop-item-special{background: white;padding: 12px;color: #999;border-bottom:4px solid #f5f5f5;padding-left: 50px;}
.shop-remark{border-bottom: 10px solid #f5f5f5;padding: 10px 15px;background: white;}
.shop-remark text{font-size: 14px;line-height: 24px;}
.shop-remark input{color: #1A1A1A;margin-left: 10px; z-index: 0;}
.shop-remark image{width: 18px;height: 18px;line-height: 45px; text-align: right;margin-top: 5px;}
.shop-name-unit{ font-size: 13px; color: #999;}
.shop-name-unit font,.shop-name-unit text{ color: #33b0ff;}
.add-del-icon{ height: 80px; line-height: 80px;}
.add-del-icon image{ vertical-align: middle; margin-top: -3px;}
.add-del-icon text{ padding: 0px 10px; }
.shop-name-text image{ width: 20px; height: 20px; vertical-align: middle; margin-top: -4px; margin-left: 5px;}
.down-icon{ width: 25px; height: 25px;margin-top: 30px;}
.sure-button{ height: 45px; line-height: 45px; background-color: #fff; padding: 0px 12px;}
.total-text{ text-align: right; padding-right: 12px;}
.total-text font{ font-size: 13px;}
.total-text text{ font-size: 16px;}
.total-text font,.total-text text{ color: #33b0ff;}
.sure-box{ position: fixed; bottom: 0px; right: 0px; left: 0px;}
.select-icon text{ color: #999;}
.sure-view-button{ background-color: #33b0ff; color: #fff; height: 30px; line-height: 30px; border-radius: 35px; padding: 0px 18px; margin-top: 7px;}
.shop-item-box{ position: relative; width: 100%;}
.shop-delete-botton{ height: 104px; width: 80px; line-height: 104px; text-align: center;color: #fff; background-color: red; right: -80px; top: 0px; position: absolute;}
.ml-80{ margin-left: -80px;}

.over-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: fixed;left: 0px; top: 0;z-index: 1;}
.special-box{ position: fixed; bottom: 0px; left: 0px; right: 0px; background-color: #fff; max-height: 75%; overflow-y: auto; font-size: 14px; z-index: 2}
.special-box image{width:16px;height: 16px;margin-top: 3px;}
.special-box-item{padding: 18px 12px;}
.special-shop-name{font-size: 16px;font-weight: bold;color: #1A1A1A;margin-right: 12px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.special-shop-normal{font-size: 15px;font-weight: bold;color: #666666;line-height: 26px;}
.special-shop-price{font-size: 17px;font-weight: bold;color: #33b0ff;}
.special-shop-unit{font-size: 12px;font-weight: bold;color: #999999;margin-top: 3px;}
.special-shop-unit2{font-size: 15px;color: #1A1A1A;line-height: 26px;}
.special-box button{font-size: 16px;font-weight: bold;}
.special-button-none{background: #d1d1d1;color: white;}
.special-button-active{background: #33b0ff;color: white;}
.addNum{width: 26px;height: 26px;background: #f5f5f5;border-left: 1px solid #e9e9e9;font-size: 18px;text-align: center;line-height: 26px;font-weight: bold;color: #b3b3b3;}
.cutNum{width: 26px;height: 26px;background: #f5f5f5;border-right: 1px solid #e9e9e9;font-size: 18px;text-align: center;line-height: 26px;font-weight: bold;color: #b3b3b3;}
.orderNum{border: 1px solid #e9e9e9;border-radius: 4px;height: 28px;}
.orderNum input{width: 70px;height:26px;font-size: 15px;color: #33b0ff;text-align: center;}

.picker-box{width: 300px;height: 250px;background: white;position: fixed;top: 50%;left: 50%;margin-top: -125px;margin-left: -150px;border-radius: 6px;z-index: 2;}
.picker-box-title{background: #33b0ff;color: white;font-size: 16px;border-radius: 6px 6px 0 0;text-align: center;line-height: 50px;font-weight: bold;}
.picker-box-area{height: 160px;text-align:center;}
.picker-box-btn{text-align: center;font-size: 16px;border-top: 1px solid #f1f1f1;line-height: 40px;font-weight: bold;}
.btn-cancle{color: #a1a1a1;}
.btn-comfirm{color: #33b0ff;}



.ck_tips{position: fixed; left: 50%;top: 40%;z-index: 100000; margin-left: -100px; margin-top: -18px;}
.tips_box{position: relative;width:200px; min-height: 30px; }
.tips_bg{ width: 100%;  background-color: #000;opacity: 0.6;border-radius: 8px;position: absolute}
.tips_text{ width: 100%; text-align: center; line-height: 30px;position: absolute;z-index: 10000000000000; color: #fff;background-color: #000;max-height: 90px;overflow: hidden;border-radius: 8px;}


.qty{display: inline-block;width: 50px;height: 20px;border-radius: 6px;vertical-align: middle;text-align: center;margin: 0px 5px;margin-top: -3px;}

.emtry-view{height: 100%;text-align: center;}
.emtry-view image{width: 80px; height: 100px;margin-top: 150px;margin-bottom: 20px;}
.emtry-view text{color: #999999;}