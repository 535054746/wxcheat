@import "../common/page/footer.wxss";
.mycontainer { box-sizing: border-box;font-size: 14px;}
.order-main{ }
.order-table{ padding-top: 41px; }
.type-header{ padding: 0px 12px; background-color: #fff; display: flex;}
.type-header-box{ overflow-y: auto;}
.current-type{color: #33b0ff;font-weight: bold}
.down-icon{ width: 25px; height: 25px;margin-top: 30px;}
.search-icon{ width: 18px; height: 18px;margin-top: 14px; margin-left: 10px;}
.shop-type{ padding: 0px 12px; color: #ee8c41; font-size: 13px;margin-top: 5px;}
.shop-item{ margin-top: 5px; border-bottom: 1px solid #f5f5f5;}
.shop-item-image{width: 80px; height: 80px;}
.shop-item-image image{ width: 80px; height: 80px; display: block;border-radius: 6px;}
.shop-item{ padding: 12px 12px; background-color: #fff;}
.shop-name-text{ margin-top: 5px;height: 38px;line-height: 120%;text-align: justify;font-size: 16px;font-weight: bold;}
.shop-item-name{ padding: 0px 12px;}
.shop-name-unit{ margin-top: 12px; font-size: 13px; color: #999;}
.special-text{background: white; padding: 5px 20px;border-bottom: solid 1px #f5f5f5;}
.edit-icon{ margin-right: 20px;}
.money_total{ position: fixed;width: 100%; left: 0px; bottom: 0px;height: 60px; background-color: #fff;}
.money_total_box{ text-align: right; padding:5px 12px; border-top:1px solid #e3e3e3; margin-top: 10px;}
.sure-button{ background-color: #33b0ff; color: #fff; text-align: center; line-height: 40px; height: 40px; border-radius: 6px; font-size: 16px;}
.shop-name-text image{ width: 18px; height: 18px; vertical-align: middle; margin-top: -4px; margin-left: 5px;}
.add-del-icon{ height: 80px; line-height: 80px;}
.add-del-icon image{ vertical-align: middle; margin-top: -3px;}
.add-del-icon text{ padding: 0px 10px;}
.shop-name-unit font{ color: #33b0ff;}
.shop-name-unit text{ color: #33b0ff; font-size: 16px;}
.company-box{background-color: #fff; padding: 8px 12px;width: 100%;position: fixed;top: 1px;border-bottom: 1px solid #E9E9E9;}
.company-name{ background-color: #33b0ff; border-radius: 10px;  height: 25px; line-height: 25px; padding: 0px 10px; color: #fff;}
.company-down-icon image{ width: 15px; height: 8px;}
.search-box{ background-color: #fff; padding: 0px 12px;height: 45px; line-height: 45px; overflow: hidden; position: fixed;top: 42px;right: 0px; left: 0px;}
.search-input{ position: relative; margin-top: 10px;}
.search-input input{ background-color: #f5f5f5; border-radius: 6px; margin-right: 12px; height: 25px; padding: 0px 12px;}
.search-input image{ position: absolute; right: 20px; top: 4px; z-index: 1;width: 18px; height: 18px;}
.cancel-button{ color: #33b0ff;}
.shop-item-rule{ background-color: #fff; color: #999; padding: 5px 12px; font-size: 13px;}
.fixed-button{ width: 100%;box-sizing: border-box;font-size: 14px;}
.move-view image{ width: 71px; height: 71px; }
.fixed-button2{ position: fixed;text-align: right; font-size: 14px;}
.share-print image{ width: 71px; height: 71px; }
.type-header-box-box{ width: 10000px;}
.type-header-box-box-box{ float: left; overflow: hidden;}
.type-header-box-box-box view{height: 45px; line-height: 45px; text-align: center; float: left; margin-right: 20px;}
.select-icon{margin-right: 7px;}
.select-icon image{ width: 15px; height: 10px;}
.company-item2{ height: 45px; line-height: 45px; padding: 0px 12px; border-top: 1px solid #f5f5f5; color: #666;}
.company-item2-press{ background: #f7f7f7;}
.company-box2{ position: fixed; top: 41px; left: 0px; right: 0px; background-color: #fff; max-height: 300px; overflow-y: auto; font-size: 14px; z-index: 1000000000}
.over-bg{ background-color: #000;opacity: 0.3; width: 100%; height: 100%;position: fixed;left: 0px; top: 1px;z-index: 1;}
.special-box{ position: fixed; bottom: 0px; left: 0px; right: 0px; background-color: #fff; max-height: 75%; overflow-y: auto; font-size: 14px; z-index: 3}
.special-box image{width:16px;height: 16px;margin-top: 3px;}
.special-box-item{padding: 18px 12px;}
.special-shop-name{max-width:70%;font-size: 16px;font-weight: bold;color: #1A1A1A;margin-right: 12px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.special-shop-normal{font-size: 15px;font-weight: bold;color: #666666;line-height: 26px;}
.special-shop-price{font-size: 17px;font-weight: bold;color: #33b0ff;}
.special-shop-unit{font-size: 12px;font-weight: bold;color: #999999;margin-top: 3px;}
.special-shop-unit2{font-size: 15px;color: #1A1A1A;line-height: 26px;}
.special-box button{font-size: 16px;font-weight: bold;}
.special-button-none{background: #d1d1d1;color: white;}
.special-button-active{background: #33b0ff;color: white;}
.addNum{width: 26px;height: 26px;background: #f5f5f5;border-left: 1px solid #e9e9e9;font-size: 18px;text-align: center;line-height: 26px;font-weight: bold;color: #b3b3b3;}
.cutNum{width: 26px;height: 26px;background: #f5f5f5;border-right: 1px solid #e9e9e9;font-size: 18px;text-align: center;line-height: 26px;font-weight: bold;color: #b3b3b3;}
.orderNum{border: 1px solid #e9e9e9;border-radius: 4px;height: 28px;}
.orderNum input{width: 70px;height:26px;font-size: 15px;color: #33b0ff;text-align: center;}
.special-group-item{float:left;height: 24px;line-height: 24px;text-align: center;border-radius: 12px;font-size: 15px;min-width: 60px;padding: 0 10px;margin: 0 5px;margin-bottom: 12px;}
.special-group{padding-right: 10px;padding-left: 5px;}
.special-group-item-normal{color: #1A1A1A;background: #f5f5f5;}
.special-group-item-checked{color: white;background: #33B0FF;}
.lines{height:1px;width: 100%;background: #f5f5f5;}
.clear{clear: both;}

.shop-special-item{background: white;padding: 6px 15px;}
.shop-special-item .shop-special-item-value{text-align: justify;font-size: 16px;line-height: 20px;}
.shop-special-item .shop-special-item-price{color: #33B0FF;font-size: 16px;line-height: 20px;}
.shop-special-item .shop-special-item-collect{background: white;width: 16px;height: 16px;vertical-align: middle;margin-left: 5px;margin-top: 3px;}
.add-del-icon-special{height: 26px;}
.add-del-icon-special image{ vertical-align: middle; margin-top: -3px;width: 20px;height: 20px;}
.add-del-icon-special text{ margin-left: 10px; margin-right: 10px;}


.on-off{color: #33B0FF;border: solid 1px #33B0FF;border-radius: 3px;padding:1px 2px; width: 60px;text-align: center;}

.qty{display: inline-block;width: 50px;height: 18px;border-radius: 6px;vertical-align: middle;text-align: center;margin:0 5px;margin-top: -3px;}



.emtry-view{height: 100%;text-align: center;}
.emtry-view image{width: 80px; height: 81px;margin-top: 200px;margin-bottom: 12px;}
.emtry-view text{color: #999999;font-size: 16px;}



.ck_tips{position: fixed; left: 50%;top: 40%;z-index: 100000; margin-left: -100px; margin-top: -18px;}
.tips_box{position: relative;width:200px; min-height: 30px; }
.tips_bg{ width: 100%;  background-color: #000;opacity: 0.6;border-radius: 8px;position: absolute}
.tips_text{ width: 100%; text-align: center; line-height: 30px;position: absolute;z-index: 10000000000000; color: #fff;background-color: #000;max-height: 90px;overflow: hidden;border-radius: 8px;}